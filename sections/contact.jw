        {% if media or form %}
            <div class="row gy-4 pd-fix">
                    {% if media %}
                    <div class="col-lg-6 col-md-12 col-sm-12 order-1 order-md-0 pd-none">
                        <div class="contact_img">
                            <img src="{{ media }}" alt="{{ media.alt }}">  
                        </div>
                    </div>
                    {% endif %}
                    
                    {% if form %}
                        <div class="col-lg-6 col-md-12 col-sm-12 background4 align-items-center order-0 order-md-1">
                            <div class="contact_stylek">
                                {{ form }}
                            </div> 
                        </div>
                    {% endif %} 
            </div>
        {% endif %}  
    </div>

    {% if map %}
        <div class="row">
        <div class="contact_map">
            <iframe src="{{ map }}"
            width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div> 
        </div>
    {% endif %}
    </section>
    
    <style>
    .contact_stylek{padding: 1.5rem;}
      .contact_stylek label{margin-bottom:0.5rem;}
    .contact_img img{width:100%; aspect-ratio: 1/1; object-fit: cover;}
    .contact_stylek input, .contact_stylek textarea{width:100%; border: none; outline:none;  padding: 1rem; border: 1px solid rgb(228, 226, 226); margin-bottom: 1rem; transition: all 0.4s linear;}
    .contact_stylek input[type=submit]{background-color: var(--color1); padding: 0.8rem; color: var(--color5); width:25%;  margin-top: 0.6rem; margin-bottom: 0;}
    .contact_stylek input:hover, .contact_stylek textarea:hover{border: 1px solid var(--color1);}
    .contact_location img{height: 60px; width:60px; object-fit: cover;}
    .contact_location_text{padding-left: 1.5rem;}
    .contact_location{display: flex; justify-content:center; align-items: center;  padding: 1.5rem; border: 1px solid rgb(228, 226, 226);}
    .contact_location img{height: 60px; width:60px; object-fit: cover;}
    .contact_location_text{padding-left: 1.5rem;}
    
    @media (max-width:576px) {
    .contact_stylek input[type=submit]{width: 40%;}
     .contact_style{padding: 1.5rem;}
      .contact_location{ justify-content:flex-start;}
      .pd-fix{padding-left:1rem;padding-right:1rem;}
      .pd-none{padding:0;}
    }
    </style>